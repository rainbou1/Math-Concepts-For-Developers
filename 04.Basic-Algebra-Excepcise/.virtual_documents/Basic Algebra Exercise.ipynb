def nat():
    """Returns all natural numbers"""
    current_number = 0
    while True:
        yield current_number
        current_number += 1

def even():
    """Returns all even numbers"""
    #Consider 0 an even number
    for number in nat():
        if number % 2 == 0:
            yield number
    


