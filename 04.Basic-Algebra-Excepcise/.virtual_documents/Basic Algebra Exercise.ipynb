def plot_complex_number(z):
    """
    Plots the complex number z as a radius vector in the 2D space
    """
    plt.quiver(0, 0, z.real, z.imag, angles = "xy", scale_units = "xy", scale = 1)
    
    plt.xticks(range(-4, 6))
    plt.yticks(range(-4, 6))
    

